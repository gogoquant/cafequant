{
  "plugins": ["react"],
  "extends": ["elemefe"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "quotes": [1, "single"],
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "no-unused-vars": 0,
    "comma-dangle": 0,
    "no-undef": 0
  }
}
